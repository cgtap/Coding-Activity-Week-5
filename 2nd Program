#include <iostream>

using namespace std;

int main() {
    
    double a;
    double b;
    double c;
    
    
    cout << "Enter your first number:";
    cin >> a;
    
    cout << "Enter your second number:";
    cin >> b;
    
    cout << "Enter your third number:";
    cin >> c;
    
    cout<<endl;
    
    if (a>b && b>c) 
        cout<<c<<" "<<b<<" "<<a;
    
    else if (a>c && c>b)
        cout<<b<<" "<<c<<" "<<a;
    
    else if (b>a && a>c)
        cout<<c<<" "<<a<<" "<<b;
    
    else if (b>c && c>a)
        cout<<a<<" "<<c<<" "<<b;
    
    else if (c>a && a>b)
        cout<<b<<" "<<a<<" "<<c;
    
    else cout<<a<<" "<<b<<" "<<c;

        
    
            
    
    return 0;
}
